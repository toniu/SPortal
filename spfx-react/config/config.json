{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "profile-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/profile/ProfileWebPart.js",
          "manifest": "./src/webparts/profile/ProfileWebPart.manifest.json"
        }
      ]
    },
    "dashboard-app-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dashboardApp/DashboardAppWebPart.js",
          "manifest": "./src/webparts/dashboardApp/DashboardAppWebPart.manifest.json"
        }
      ]
    },
    "group-management-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/groupManagement/GroupManagementWebPart.js",
          "manifest": "./src/webparts/groupManagement/GroupManagementWebPart.manifest.json"
        }
      ]
    },
    "poll-management-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/pollManagement/PollManagementWebPart.js",
          "manifest": "./src/webparts/pollManagement/PollManagementWebPart.manifest.json"
        }
      ]
    },
    "calendar-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/calendar/CalendarWebPart.js",
          "manifest": "./src/webparts/calendar/CalendarWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "ProfileWebPartStrings": "lib/webparts/profile/loc/{locale}.js",
    "DashboardAppWebPartStrings": "lib/webparts/dashboardApp/loc/{locale}.js",
    "GroupManagementWebPartStrings": "lib/webparts/groupManagement/loc/{locale}.js",
    "PollManagementWebPartStrings": "lib/webparts/pollManagement/loc/{locale}.js",
    "CalendarWebPartStrings": "lib/webparts/calendar/loc/{locale}.js"
  }
}
