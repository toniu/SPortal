{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "faq-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/faq/FaqWebPart.js",
          "manifest": "./src/webparts/faq/FaqWebPart.manifest.json"
        }
      ]
    },
    "hello-tailwind-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/helloTailwind/HelloTailwindWebPart.js",
          "manifest": "./src/webparts/helloTailwind/HelloTailwindWebPart.manifest.json"
        }
      ]
    },
    "simple-poll-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/simplePoll/SimplePollWebPart.js",
          "manifest": "./src/webparts/simplePoll/SimplePollWebPart.manifest.json"
        }
      ]
    },
    "calendar-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/calendar/CalendarWebPart.js",
          "manifest": "./src/webparts/calendar/CalendarWebPart.manifest.json"
        }
      ]
    },
    "feed-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/feed/FeedWebPart.js",
          "manifest": "./src/webparts/feed/FeedWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "FaqWebPartStrings": "lib/webparts/faq/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "HelloTailwindWebPartStrings": "lib/webparts/helloTailwind/loc/{locale}.js",
    "SimplePollWebPartStrings": "lib/webparts/simplePoll/loc/{locale}.js",
    "CalendarWebPartStrings": "lib/webparts/calendar/loc/{locale}.js",
    "FeedWebPartStrings": "lib/webparts/feed/loc/{locale}.js"
  }
}
